        "postcss": "5.2.16",
        "precss": "1.4.0",
        "postcss-smart-import": "0.6.10",
        "postcss-flexbugs-fixes": "2.1.0",
        "sass-lint": "1.10.2",
        "rimraf": "2.6.1",

        "es6-promise": "4.1.0",
        "es6-shim": "0.35.3",
        "es7-reflect-metadata": "1.6.0",

